package com.string;

public class StringDemo {

	public static void main(String[] args) {
		String s = new String("Mohan");
		String s1 = new String("Mohan");
		String s3 = "Mohan";
		String s4 = "Mohan";
		s.concat("Rath");
		System.out.println(s == s1);
		System.out.println(s.equals(s1));
		System.out.println("String : "+s);
		System.out.println(s4 == s3);
		System.out.println(s4.equals(s3));
		
		StringBuffer sb = new StringBuffer("Mohan ");
		StringBuffer sb1 = new StringBuffer("Mohan ");
		sb.append("Rath");
		System.out.println("String Buffer : "+sb);
		System.out.println(sb == sb1);
		System.out.println(sb.equals(sb1));
		
		
		String r = new String("Mohan ");
		r.concat("Chand");
		
		String r1 = r.concat("Chand ");
		r1.concat("Software Engineer");
		
		r = r1.concat("Rath");
		
		System.out.println(r);
		System.out.println(r1);
	}

}
